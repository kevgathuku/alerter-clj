{:aliases
 {:dev
  {:extra-paths ["dev"]
   :extra-deps {nrepl/nrepl {:mvn/version "1.3.0"}}}

  :fmt
  {:extra-deps {cljfmt/cljfmt {:mvn/version "0.9.2"}}
   :main-opts ["-m" "cljfmt.main" "fix"]}

  :lint
  {:deps {clj-kondo/clj-kondo {:mvn/version "2025.12.23"}}
   :main-opts ["-m" "clj-kondo.main" "--lint" "src"]}}}

